[core]
	editor = "nvim"
[init]
	defaultBranch = main
[alias]
	st = "status"
	sw = "switch"
	ps = "push origin HEAD"
	plog = "log -p"
	graph = "log --oneline --graph"
	pushf = "push --force-with-lease --force-if-includes origin HEAD"
	ndf = "!git diff --no-merges origin/$(git rev-parse --abbrev-ref HEAD)..HEAD"
	nlg = "!git log --no-merges origin/$(git rev-parse --abbrev-ref HEAD)..HEAD"
[rebase]
	autostash = true
